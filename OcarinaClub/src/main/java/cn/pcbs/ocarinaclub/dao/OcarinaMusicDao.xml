<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="cn.pcbs.ocarinaclub.dao.OcarinaMusicDao" >
  	
  	<select id="getMusicByMusician" resultType="cn.pcbs.ocarinaclub.model.OcarinaMusic">
  		SELECT * FROM t_ocarinamusic WHERE `musicianName` = #{musicianName}
  	</select>
  	
  	<select id="getAllMusic" resultType="cn.pcbs.ocarinaclub.model.OcarinaMusic">
  		SELECT * FROM t_ocarinamusic
  	</select>
 </mapper>