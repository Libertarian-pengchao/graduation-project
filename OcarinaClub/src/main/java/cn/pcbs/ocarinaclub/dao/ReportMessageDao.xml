<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="cn.pcbs.ocarinaclub.dao.ReportMessageDao" >
  	
  	<insert id="insertReportMessage">
  		insert into `t_reportmessage` 
  			(id,`reporterName`,`title`,`content`,`isDeal`,`reportPostName`) 
  				values (id,#{reporterName},#{title},#{content},#{isDeal},#{reportPostName})
  	</insert>
 </mapper>